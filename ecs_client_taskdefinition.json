{
    "containerDefinitions": [
      {
        "name": "client",
        "image": "%s.dkr.ecr.us-east-1.amazonaws.com/experimental-flask-react-client:prod",
        "essential": true,
        "memoryReservation": 300,
        "portMappings": [
          {
            "hostPort": 0,
            "protocol": "tcp",
            "containerPort": 80
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "flask-react-client-log",
            "awslogs-region": "us-east-1"
          }
        }
      }
    ],
    "family": "experimental-flask-react-client-td-1"
  }